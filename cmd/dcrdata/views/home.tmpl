{{define "voteTable"}}
<div class="row">
   <div class="col-12"><span class="medium-sans">Ayes: &nbsp;</span>&nbsp;{{.Aye}}</div>
   <div class="col-12"><span class="medium-sans">Approved: &nbsp;</span>&nbsp;{{printf "%.1f" (f32x100 .Approval)}}%</div>
   <div class="col-12"><span class="medium-sans">Nays: &nbsp;</span>&nbsp;{{.Nay}}</div>
   <div class="col-12"><span class="medium-sans">Abstain: &nbsp;</span>&nbsp;{{.Abstain}} ({{printf "%.1f" (f32x100 .AbstainRate)}}%)</div>
</div>
{{end}}
{{define "home" -}}
