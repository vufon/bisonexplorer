{{define "html-head"}}
<head data-turbolinks-eval="false">
		<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{{/* The above meta tags *must* come first in the head */ -}}
	{{- if .Data.Host -}}
	<meta name="description" content="Bison Explorer, an original Multi-chain explorer powered by Go">
	<meta name="author" content="The mutilple blockchains developers">
	<meta name="application-name"           content="Bison Explorer - The Mutilple Blockchain Explorer">
