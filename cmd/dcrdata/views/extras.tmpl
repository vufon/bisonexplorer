				</td>
				<td class="text-end">
					{{dateTimeWithoutTimeZone .Time}}
				</td>
			</tr>
			{{end}}
			<!-- End Source redemptions -->
			{{if $hasTargetToken}}
			{{ $targetToken := .TargetToken }}
			<!-- Begin Target contracts -->
			{{- range .Target.Contracts}}
